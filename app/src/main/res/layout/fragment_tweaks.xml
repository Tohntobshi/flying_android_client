<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" >
    <data>
        <variable
            name="viewmodel"
            type="com.example.flyingandroidclient.MainActivityViewModel" />
    </data>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".TweaksFragment"
        tools:background="@color/cardview_dark_background" >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Preset"
                android:textAlignment="center"
                android:gravity="center_horizontal"
                android:textSize="25dp"
                android:layout_marginTop="20dp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >
                <Button
                    android:layout_weight="1"
                    android:layout_height="40dp"
                    android:layout_margin="5dp"
                    android:text="Restore"
                    android:onClick="@{() -> viewmodel.controls.restoreSettings()}"
                    android:layout_width="wrap_content" />
                <Button
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_margin="5dp"
                    android:text="Save"
                    android:onClick="@{() -> viewmodel.controls.saveCurrentSettings()}" />
                <Button
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_margin="5dp"
                    android:text="Send"
                    android:onClick="@{() -> viewmodel.controls.sendCurrentSettings()}" />

            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Safety settings"
                android:textAlignment="center"
                android:gravity="center_horizontal"
                android:textSize="25dp"
                android:layout_marginTop="50dp" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="10dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Turn off angle"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderTurnOffInclineAngle"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.turnOffInclineAngle}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="1"
                        app:fractionDigits="1"
                        app:minValue="0" />
                </LinearLayout>
                <Button
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_margin="5dp"
                    android:text="Reset turn off"
                    android:layout_gravity="bottom"
                    android:onClick="@{() -> viewmodel.controls.resetTurnOffTrigger()}" />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Incline settings"
                android:textAlignment="center"
                android:gravity="center_horizontal"
                android:textSize="25dp"
                android:layout_marginTop="50dp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_margin="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="&lt;B Pitch Adjust F&gt;"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp"
                        android:layout_marginTop="10dp"/>
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderPitchAdjust"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.pitchAdjust}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_margin="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="&lt;L Roll Adjust R&gt;"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp"
                        android:layout_marginTop="10dp"/>
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderRollAdjust"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.rollAdjust}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_margin="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@{`Pitch err ` + viewmodel.pitchErr}"
                        tools:text="Pitch err"
                        android:textAlignment="center"
                        android:textSize="16dp"
                        android:gravity="center_horizontal" />
                    <com.example.flyingandroidclient.Plot
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        app:plotData="@{viewmodel.pitchErrors}"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="Pitch err der"
                        android:text="@{`Pitch err der ` + viewmodel.pitchErrDer}"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp"/>
                    <com.example.flyingandroidclient.Plot
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        app:plotData="@{viewmodel.pitchErrorChangeRates}" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="Pitch err int"
                        android:text="@{`Pitch err int ` + viewmodel.pitchErrInt}"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Pitch P I D"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp"
                        android:layout_marginTop="10dp"/>
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderPitchProp"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.pitchPropCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0"/>
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderPitchInt"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.pitchIntCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0"/>
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderPitchDer"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.pitchDerCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_margin="10dp"
                    >
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@{`Roll err ` + viewmodel.rollErr}"
                        tools:text="Roll err"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Plot
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        app:plotData="@{viewmodel.rollErrors}" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="Roll err der"
                        android:text="@{`Roll err der ` + viewmodel.rollErrDer}"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Plot
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        app:plotData="@{viewmodel.rollErrorChangeRates}" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="Roll err int"
                        android:text="@{`Roll err int ` + viewmodel.rollErrInt}"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Roll P I D"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp"
                        android:layout_marginTop="10dp"/>
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderRollProp"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.rollPropCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0"/>
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderRollInt"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.rollIntCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0"/>
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderRollDer"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.rollDerCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0"/>
                </LinearLayout>


            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Incline filtering"
                android:textAlignment="center"
                android:gravity="center_horizontal"
                android:textSize="25dp"
                android:layout_marginTop="50dp" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_margin="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Accel trust"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderAccTrust"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.accTrust}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0"
                        app:maxValue="1"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Incline Filtering"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderInclFilt"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.inclineFilteringCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0"
                        app:maxValue="1"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_margin="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="IMU LPF mode"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderImuLPFMode"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.imuLPFMode}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="1"
                        app:fractionDigits="0"
                        app:minValue="1"
                        app:maxValue="6"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Incline Der Filtering"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderInclChRFilt"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.inclineChangeRateFilteringCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0"
                        app:maxValue="1"/>
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Height settings"
                android:textAlignment="center"
                android:gravity="center_horizontal"
                android:textSize="25dp"
                android:layout_marginTop="50dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_margin="10dp" >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@{`Height err ` + viewmodel.heightErr}"
                        tools:text="Height err 0"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Plot
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        app:plotData="@{viewmodel.heightErrors}"
                        app:valueRange="1.0" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@{`desired height ` + viewmodel.controls.acceleration * 4.0f}"
                        tools:text="desired height 0"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Base acceleration"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderBaseAccel"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.baseAcceleartion}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0"
                        app:maxValue="1"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Height P"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderHeightProp"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.heightPropCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_margin="10dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@{`Height err der ` + viewmodel.heightErrDer}"
                        tools:text="Height err der 0"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp"/>
                    <com.example.flyingandroidclient.Plot
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        app:plotData="@{viewmodel.heightErrorChangeRates}"
                        app:valueRange="4.0" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@{`Height err int ` + viewmodel.heightErrInt}"
                        tools:text="Height err int 0"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Height I"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderHeightInt"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.heightIntCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Height D"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderHeightDer"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.heightDerCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0" />
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Yaw settings"
                android:textAlignment="center"
                android:gravity="center_horizontal"
                android:textSize="25dp"
                android:layout_marginTop="50dp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="20dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="Yaw sp err"
                        android:text="@{`Yaw sp err ` + viewmodel.yawSpErr}"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp"/>
                    <com.example.flyingandroidclient.Plot
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        app:plotData="@{viewmodel.yawSpErrors}"
                        app:valueRange="90.0" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="Yaw sp err int"
                        android:text="@{`Yaw sp err int ` + viewmodel.yawSpErrInt}"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Yaw sp Filter"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderYawSpFilt"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.yawSpFilteringCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="20dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Yaw sp P I D"
                        android:textAlignment="center"
                        android:gravity="center_horizontal"
                        android:textSize="16dp" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderYawSpProp"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.yawSpPropCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderYawSpInt"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.yawSpIntCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0" />
                    <com.example.flyingandroidclient.Slider
                        android:id="@+id/sliderYawSpDer"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:position="@{viewmodel.controls.yawSpDerCoef}"
                        app:orientation="horizontal"
                        app:positionChangeMultiplier="0.1"
                        app:fractionDigits="3"
                        app:minValue="0" />
                </LinearLayout>

            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Other info"
                android:textAlignment="center"
                android:gravity="center_horizontal"
                android:textSize="25dp"
                android:layout_marginTop="50dp"/>
            <LinearLayout
                android:layout_width="match_parent"
